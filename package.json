{
    "name": "v3d-platform",
    "version": "1.0.0",
    "private": true,
    "description": "V3D Platform - Enterprise 3D Visualization Platform",
    "scripts": {
        "dev": "pnpm --parallel -r dev",
        "build": "pnpm -r build",
        "test": "pnpm -r test",
        "lint": "pnpm -r lint",
        "clean": "pnpm -r clean",
        "db:migrate": "cd packages/database && pnpm exec prisma migrate dev",
        "db:studio": "cd packages/database && pnpm exec prisma studio",
        "db:generate": "cd packages/database && pnpm exec prisma generate"
    },
    "keywords": [
        "3d",
        "visualization",
        "real-estate",
        "enterprise"
    ],
    "author": "MAGAMA",
    "license": "UNLICENSED",
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "typescript": "^5.3.0",
        "prettier": "^3.1.0",
        "eslint": "^8.55.0"
    }
}